{
	"name": "arcanist-vscode",
	"displayName": "arcanist-vscode",
	"description": "arcanist plugin for vscode",
	"publisher": "jdxcode",
	"version": "1.0.2",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/jdxcode/arcanist-vscode"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "arc.diff.create",
				"title": "Create diff",
				"category": "arc"
			},
			{
				"command": "arc.diff",
				"title": "diff",
				"category": "arc"
			},
			{
				"command": "arc.diff.update",
				"title": "Update diff",
				"category": "arc"
			}
		],
		"configuration": {
			"title": "Arcanist",
			"properties": {
				"arcanist.pathToArc": {
					"type": "string",
					"description": "Path to arc bin"
				},
				"arcanist.debug": {
					"type": "boolean",
					"default": false,
					"description": "Show debug output"
				}
			}
		}
	},
	"scripts": {
		"compile": "tsc -p ./",
		"package": "vsce package",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "webpack --mode production",
		"watch": "tsc -watch -p ./",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch"
	},
	"devDependencies": {
		"@types/execa": "^2.0.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.17.3",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"prettier": "^1.18.2",
		"ts-loader": "^6.2.1",
		"tslint": "^5.12.1",
		"typescript": "^3.7.2",
		"vsce": "^1.69.0",
		"vscode-test": "^1.2.2",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"execa": "^3.2.0"
	},
	"__metadata": {
		"id": "39ef8ce8-4827-45b2-9eb0-816d467c6f59",
		"publisherDisplayName": "jdxcode",
		"publisherId": "51834022-b041-42bf-ace9-576c4005bbd5"
	}
}
